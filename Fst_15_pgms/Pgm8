class Sumavg
 {
   public static void main(String args[])
   { 
    
     int sum = 0;
     int avg = 0;
     for(int i=1;i<=5;i++)
     {
       sum = sum + i;
       
     }
     avg = sum / 5;
    System.out.println("sum of 5 numbers"+sum);
    System.out.println("average of 5 numbers"+avg);
   }
 }
